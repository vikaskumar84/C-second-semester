#include <stdio.h>

int main()
{
    int num,f=0;
    scanf("%d",&num);
    for(int i=1;i<num;i++){
        f=i*i*i;
        if(f==num){
            printf("perfect cube \n");
            return 0;
        }
    }
    printf("not perfect cube");
    
}

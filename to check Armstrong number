#include <stdio.h>

int main()
{
    int num,d,sum=0,a;
    scanf("%d",&num);
        a=num;

    while(num>0){
        d=num%10;
        sum+=d*d*d;
        num=num/10;
        printf("%d\n",sum);
        
    }
    if(a==sum){
        printf("Armstrong \n");
    }
    else{
        printf("Not Armstrong \n");
    }

    return 0;
}

#include <stdio.h>

int main()
{
    int num,d,rev=0,a;
    scanf("%d",&num);
        a=num;

    while(num>0){
        d=num%10;
        rev=rev*10+d;
        num=num/10;
        
    }
    if(a==rev){
        printf("Palindrome \n");
    }
    else{
        printf("Not palindrome \n");
    }

    return 0;
}

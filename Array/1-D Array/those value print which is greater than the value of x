#include<stdio.h>
int main(){
    int a,x=4,count=0;
    printf("enter the elements of the array ");
    scanf("%d",&a);
    int arr[a];
    for(int i=0;i<a;i++){
        scanf("%d",&arr[i]);
    }
    for(int i=0;i<a;i++){
        if(arr[i]>x){
            count+=1;
            printf("indexing of %d is %d\n",arr[i],i);
        }
    }
    printf("count= %d\n",count);

}
    


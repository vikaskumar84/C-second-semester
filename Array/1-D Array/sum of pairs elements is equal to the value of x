#include<stdio.h>
int main(){
    int a,x=12;
    printf("enter the elements of the array ");
    scanf("%d",&a);
    int arr[a];
    for(int i=0;i<a;i++){
        scanf("%d",&arr[i]);
    }
    for(int i=0;i<a;i++){
        for(int j=i+1;j<a;j++){
            if(arr[i]+arr[j]==x){
                printf("(%d,%d) ",arr[i],arr[j]);

            }
        }
    }


}

    


#include<stdio.h>
int main(){
    int a;
    printf("enter the elements of the array");
    scanf("%d",&a);
    int arr[a];
    for(int i=0;i<a;i++){
        scanf("%d",&arr[i]);
    }
    for(int i=0;i<a;i++){
        if(i%2==0){
            arr[i]+=10;
        }
        else{
            arr[i]*=2;
        }
    }

for(int i=0;i<a;i++){
    printf("%d ",arr[i]);

}

}
    

